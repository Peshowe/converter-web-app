# nginx.default


server {
    listen 80;
    server_name 116.203.239.29;
    
    location / {
        proxy_pass http://127.0.0.1:8010;
        proxy_set_header Host $host;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    }
    location /static {
        root /opt/app/converter_web_app/converter;
    }
}
